CREATE TABLE "orders" (
	"shipment_id"	TEXT NOT NULL,
	"dispatchByDate"	TEXT,
	"dispatchAfterDate"	TEXT,
	"sku"	TEXT,
	"order_id"	TEXT,
	"orderItemId"	TEXT NOT NULL,
	"orderDate"	TEXT,
	"paymentType"	TEXT,
	"hsn"	TEXT,
	"priceComponents"	REAL,
	"customerPrice"	REAL,
	"flipkartDiscount"	REAL,
	"quantity"	INTEGER,
	"label"	BLOB,
	"Tracking_ID"	TEXT,
	"pincode"	NUMERIC,
	"city"	TEXT,
	"state"	TEXT,
	"stateCode"	TEXT, status TEXT,
	PRIMARY KEY("shipment_id","orderItemId")
)